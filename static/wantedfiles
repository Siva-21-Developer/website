       .box-container
        {
            height:100vh;
            position: relative;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-wrap: wrap;
            gap: 60px;
        }
        .box-container .box
        {
            position: relative;
            width: 300px;
            height: 350px;
            background: #2e2e2e;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .box-container .box::before
        {
            content: '';
            position: absolute;
            inset: -10px 50px;
            border-top: 4px solid var(--clr);
            border-bottom: 4px solid var(--clr);
            z-index: -1;
            transform: skewY(15deg);
            transition: 0.5s ease-in-out;
        }
        .box-container .box:hover::before
        {
            transform: skewY(0deg);
            inset: -10px 40px;
        }
        .box-container .box::after
        {
            content: '';
            position: absolute;
            display:none;
            inset: 60px -10px;
            border-left: 4px solid var(--clr);
            border-right: 4px solid var(--clr);
            z-index: -1;
            transform: skew(15deg);
            transition: 0.5s ease-in-out;
        }
        .box-container .box:hover::after
        {
            display:block;
            border-left-color: white;
            border-right-color:white;
            transform: skew(0deg);
            inset: 40px -10px;
        }
        .box-container .box .content
        {
            position: relative;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            text-align: center;
            gap: 20px;
            padding: 0 0px;
            width: 100%;
            height: 100%;
            overflow: hidden;
        }

        .box-container .box .content img
        {
            width:110%;
            height:110%;
            filter: grayscale(100%);
            z-index:-10;
        }
        .box-container .box .content img:hover
        {
            filter: grayscale(0%);
            transition: 0.5s ease-in-out;
        }



